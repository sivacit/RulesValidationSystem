[
  {
    "WorkflowName": "CSVWorkflow",
    "Rules": [
      {
        "RuleName": "AgeCheck",
        "RuleExpressionType": "LambdaExpression",
        "Expression": "Convert.ToInt32(input1.Age) >= 18",
        "ErrorMessage": "Patient must be at least 18 years old",
        "Enabled": true
      },
      {
      "RuleName": "HighBPCheck",
      "Expression": "Convert.ToInt32(input1.BloodPressure) < 150",
      "SuccessEvent": "Normal",
      "ErrorMessage": "High Blood Pressure Detected, Consult doctor",
      "Enabled": true
      }
    ]
  }
]